TabAuto:AddToggle({
	Name = "Bring DF",
	Default = false,
	Callback = function(BDF)
		BringDF = BDF
	end    
})

spawn(function()
    while wait() do
        if BringDF then
            pcall(function()
                for i,v in pairs(game.Workspace.Trees.Tree.Model:GetChildren()) do 
                    if v.ClassName == "Tool" then 
                        fireclickdetector(v.Main.ClickDetector)
                    end
                end
            end)
        end
    end
 end)

